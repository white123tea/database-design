<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.vueback1.mapper.StationViewMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.example.vueback1.entity.StationView">
        <result column="station_id" property="stationId" />
        <result column="station_name" property="stationName" />
        <result column="station_addr" property="stationAddr" />
        <result column="name" property="name" />
        <result column="phone" property="phone" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        station_id, station_name, station_addr, name, phone
    </sql>

</mapper>
